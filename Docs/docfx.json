{
  "metadata" : [
    {
      "src": [
        {
          "files": [
            "Src/numl/numl.csproj"
          ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "cwd" : ".."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
        {
          "files": ["api/**/*.yml"]
        },
        {
          "files": [
            "docs/*.md",
            "api/*.md"
          ]
        },
        {
          "files": ["toc.yml"]
        }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "data/**"
        ]
      }
    ],
    "externalReference": [],
    "globalMetadata" : {
        "_appTitle": "numl"
    },
    "dest": "_site",
    "template": [ "default", "template"]
  }
}